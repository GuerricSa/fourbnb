<%= simple_form_for oven do |f| %>
    <%= f.input :title %>
    <%= f.input :brand %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.button :submit, class: "btn-cta" %>
  <% end %>
